{
  "manifest_version": 2,
  "name": "Discord Message Prepender",
  "version": "1.0",
  "description": "Prepends a string to your discord messages.",
  "permissions": [
    "tabs",
    "activeTab",
	"storage",
    "https://discord.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_title": "Discord Message Prepender",
	"default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
